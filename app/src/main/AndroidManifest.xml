<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jim.demo1" >

<uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/phonemug"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".profile_activity"
            android:label="@string/ProfileButton" >
        </activity>
        <activity
            android:name=".post_activity"
            android:label="@string/PostButton" >
        </activity>
        <activity
            android:name=".matches_activity"
            android:label="@string/MatchButton" >
        </activity>
        <activity
            android:name=".app_settings_activity"
            android:label="@string/profile_app_settings_button" >
        </activity>
        <activity
            android:name=".contact_activity"
            android:label="@string/contact_text" >
        </activity>
        <activity
            android:name=".CreateUser"
            android:label="@string/title_activity_create_user" >
        </activity>
    </application>

</manifest>
